<h3>Indiviual customer page</h3>
<table class="table">
  <thead>
    <tr>
      <th>Customer</th>
      <th>Email</th>
      <th>Shopify ID</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @customer.each do |cust| %>
      <tr>
        <td><%= cust.first_name %> <%= cust.last_name %></td>
        <td><%= cust.email %></td>
        <td><%= cust.shopify_customer_id %></td>
      </tr>
    <% end %>
  </tbody>
</table>
